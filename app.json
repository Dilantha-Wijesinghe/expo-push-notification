      "package": "com.hasithgenesis.expopushnotification",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c61af7ad-948d-48dd-abbf-e704454a8934"
      }
    }
